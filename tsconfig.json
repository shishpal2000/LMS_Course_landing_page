{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/course-landing-page/WhoseItFor.js",
    "src/app/course-landing-page/SyllabusCurriculum.js",
    "src/app/course-landing-page/SyllabusCard.js",
    "src/app/course-landing-page/Skills.js",
    "src/app/course-landing-page/[slug]/page.js",
    "src/app/course-landing-page/Keyfeatures.js",
    "src/app/course-landing-page/Highlights.js",
    "src/app/course-landing-page/HeroSection.js",
    "src/app/course-landing-page/Gains.js",
    "src/app/course-landing-page/FlowChart.js",
    "src/app/course-landing-page/FAQs.js",
    "src/app/course-landing-page/ElementaSchool.js",
    "src/app/components/Tools/Tools.js",
    "src/app/components/GetCerftified/GetCertified.js",
    "src/app/paymentsuccessfull/page.js",
    "src/app/loginpage/page.js",
    "src/app/components/Loginpage/LoginPage.js",
    "src/app/components/Paymentsuccessfull/PaymentSucessFull.js",
    "src/app/components/PaymntFail/PaymentFail.js"
  ],
  "exclude": ["node_modules"]
}
